[alias]
    st = status
    ci = commit
    cam = commit --amend --no-edit
    br = branch
    rbi = rebase -i
    co = checkout
    df = diff
    dff = diff --name-only
    c-p = cherry-pick
    f-p = format-patch
    lg = log -p
    l = log --abbrev-commit --pretty=oneline
    tree = log --graph --all --decorate --pretty=oneline
    review = push origin HEAD:refs/for/master
[branch]
    autosetupmerge = true
[color]
    branch = auto
    diff = auto
    status = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[color]
    ui = true
[core]
    whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
    editor = vim 
    excludesfile = ~/.gitignore_global
[merge]
	tool = vimdiff
[push]
    default = simple
[user]
	email = szymonpajzert@gmail.com
	name = Szymon Pajzert
[diff]
	algorithm = patience
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
